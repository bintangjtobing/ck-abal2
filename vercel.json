{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1",
      "dest": "server.js"
    },
    {
      "src": "/generate",
      "dest": "server.js"
    },
    {
      "src": "/",
      "dest": "public/index.html"
    }
  ]
}